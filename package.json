    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "style-loader": "^1.1.3",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "resolutions": {
    "react-test-renderer": "^16.13.1"
  },
  "overrides": {
    "*": {
      "dependencies": {
        "react": "-"
      },
      "peerDependencies": {
        "react": "+"
      }
    }
  }
}
